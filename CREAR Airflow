##instalar
export AIRFLOW_HOME=/mnt/c/Users/<tu usuario>/Desktop/airflow
sudo pip3 install "apache-airflow==2.2.3" --constraint "https://raw.githubusercontent.com/apache/airflow/constraints-2.2.3/constraints-no-providers-3.8.txt"
#activar entorno
sudo virtualenv airflow_env
source airflow_env/bin/activate
#iniciar
airflow db init
airflow users create --username administrador --password administrador --firstname adamaris --lastname lopez --role Admin --email administradorada@nowports.com
airflow version
airflow
